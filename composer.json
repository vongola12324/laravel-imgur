{
  "name": "vongola12324/laravel-imgur",
  "description": "Laravel wrapper for the Imgur API.",
  "keywords": [
    "laravel",
    "imgur"
  ],
  "homepage": "https://github.com/vongola12324/laravel-imgur",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Vongola",
      "email": "vongola12324@coder.tw"
    }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "ext-json": "*",
    "illuminate/support": "^7.0|^8.0",
    "illuminate/http": "^7.0|^8.0",
    "guzzlehttp/guzzle": "^7.3",
    "jenssegers/model": "^1.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "squizlabs/php_codesniffer": "^3.6",
    "orchestra/testbench": "^6.0"
  },
  "autoload": {
    "psr-4": {
      "Vongola\\Imgur\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Vongola\\ImgurTests\\": "tests/"
    }
  },
  "extra": {
    "component": "package",
    "laravel": {
      "providers": [
        "Vongola\\Imgur\\ImgurServiceProvider"
      ],
      "aliases": {
        "Imgur": "Vongola\\Imgur\\Facades\\ImgurFacade"
      }
    }
  }
}
